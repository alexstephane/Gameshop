 <%= form_for @game do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :developer %>
    <%= f.text_field :developer %>
    <%= f.label :release_date %>
    <%= f.text_field :release_date %>
    <%= f.label :rating %>
    <%= f.text_field :rating %>
    <%= f.submit %>
<% end %>