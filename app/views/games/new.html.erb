 <%= form_for @game do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :developer %>
    <%= f.text_field :developer %>
    <br>
    <%= f.label :release_date %>
    <%= f.text_field :release_date %>
    <br>
    <%= f.label :rating %>
    <%= f.text_field :rating %>
    <br><br>
    <%= f.submit "Add Game" %>
<% end %>