 
 <h1>Sign Up</h1>
 <%= form_for @user do |f| %>
   <p> <%= f.label :name %>
    <%= f.text_field :name %></p>
    
   <p> <%= f.label :username%>
    <%= f.text_field :username%></p>

   <P> <%= f.label :password%>
    <%= f.password_field :password%></P> 

   <P> <%= f.label :Age %>
    <%= f.text_field :age %></P>
    
    <%= f.submit "create Account" %>
<% end %>

